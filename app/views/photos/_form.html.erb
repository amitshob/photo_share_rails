  <div class="container">
      <div class="column-center">
        <%= form_for @photo, {multipart: true} do |f| %>
          <%= f.label :image %><br>
          <%= f.file_field :image %><br>
          <%= f.label :title %><br>
          <%= f.text_field :title %><br>
          <%= f.label :description %><br>
          <%= f.text_field :description %>

          <%= f.submit  %>
        <% end %>
      </div>
    <div class="column-left">



    </div>
  </div>
